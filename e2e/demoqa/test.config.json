{
  "page": {
    "URL": "http://demoqa.com/registration/",
    "title": "Registration | Demoqa",
    "elements": [
      {
        "type": "input",
        "methods": [
          "enter",
          "get"
        ],
        "selectors": {
          "firstName": "#name_3_firstname",
          "lastName": "#name_3_lastname"
        }
      },
      {
        "type": "button",
        "methods": [
          "click"
        ],
        "selectors": {
          "submit": "input[type='submit'][value='Submit']"
        }
      },
      {
        "type": "radio",
        "methods": [
          "set", "is", "get"
        ],
        "selectors": {
          "martialStatusSingle": "input[type='radio']:nth-child(2)",
          "martialStatusMarried": "input[type='radio']:nth-child(4)",
          "martialStatusDivorced": "input[type='radio']:nth-child(6)"
        }
      },
      {
        "type": "checkbox",
        "methods": [
          "set", "unset", "is"
        ],
        "selectors": {
          "hobbyDance": "input[type='checkbox']:nth-child(2)",
          "hobbyReading": "input[type='checkbox']:nth-child(4)",
          "hobbyCricket": "input[type='checkbox']:nth-child(6)"
        }
      }
    ]
  },
  "people": [
    {
      "firstName": "Andrzej",
      "lastName": "Janusz"
    }
  ]
}