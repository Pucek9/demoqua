{
  "page": {
    "URL": "http://demoqa.com/registration/",
    "title": "Registration | Demoqa",
    "elements": [
      {
        "type": "input",
        "methods": [
          "enter",
          "get"
        ],
        "selectors": {
          "firstName": "#name_3_firstname",
          "lastName": "#name_3_lastname",
          "phoneNumber": "#phone_9",
          "username": "#username",
          "email": "#email_1",
          "photo": "#profile_pic_10",
          "description": "#description",
          "password": "#password_2",
          "confirmPassword": "#confirm_password_password_2"
        }
      },
      {
        "type": "label",
        "methods": [
          "getText",
          "is"
        ],
        "selectors": {
          "strengthIndicator": "#piereg_passwordStrength",
          "successMessage": ".piereg_message",
          "errorMessage": ".piereg_login_error"
        }
      },
      {
        "type": "button",
        "methods": [
          "click"
        ],
        "selectors": {
          "submit": "input[type='submit'][value*='Submit']"
        }
      },
      {
        "type": "radio",
        "methods": [
          "choose",
          "check"
        ],
        "selectors": {
          "martialStatus": "#pie_register > li:nth-child(2) div.radio_wrap"
        }
      },
      {
        "type": "checkbox",
        "methods": [
          "set",
          "unset",
          "is"
        ],
        "selectors": {
          "hobbyDance": "input[type='checkbox'][value*='dance']",
          "hobbyReading": "input[type='checkbox'][value*='reading']",
          "hobbyCricket": "input[type='checkbox'][value*='cricket']"
        }
      },
      {
        "type": "select",
        "methods": [
          "is",
          "get",
          "select"
        ],
        "selectors": {
          "country": "#dropdown_7",
          "dateOfBirthMonth": "#mm_date_8",
          "dateOfBirthDay": "#dd_date_8",
          "dateOfBirthYear": "#yy_date_8"
        }
      }
    ]
  },
